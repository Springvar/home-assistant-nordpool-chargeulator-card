{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Node",
    "experimentalDecorators": true,
    "useDefineForClassFields": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
 "include": [
  "src/**/*.ts",
  "src/**/*.d.ts",
  "*.d.ts"
], "exclude": ["node_modules"]
}
